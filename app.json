{
  "expo": {
    "name": "FinZen",
    "slug": "FinZen",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-image-picker"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
